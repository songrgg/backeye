{
    "name": "Post API",
    "desc": "post API",
    "cron": "*/2 * * * *",
    "watches": [
        {
            "name": "post list",
            "desc": "post list",
            "interval": 0,
            "path": "https://api-prod.wallstreetcn.com/apiv1/content/articles",
            "method": "GET",
            "headers": {
                "User-Agent": "backeye"
            },
            "assertions": [
                {
                    "source": "header",
                    "operator": "equal",
                    "left": "status_code",
                    "right": "200"
                },
                {
                    "source": "body",
                    "operator": "equal",
                    "left": "code",
                    "right": "20000"
                }
            ]
        }
    ]
}
